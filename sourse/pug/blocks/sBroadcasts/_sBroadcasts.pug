				.row

					.col-xxl-6.col-xl-5 
						+e.caption.resp-tabs-list
							+e.btn.resp-tab-btn КОНФЕРЕНЦИИ 
							+e.btn.resp-tab-btn ПРЕЗЕНТАЦИИ 
							+e.btn.resp-tab-btn церемония награждения
							+e.btn.resp-tab-btn ВЕБИНАРЫ

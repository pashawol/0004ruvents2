mixin sPrices()
	// start sPrices
	+b.SECTION.sPrices.section#sPrices&attributes(attributes)
		.container
			+b.section-title--lg
				h2 Стоимость организации трансляций
			+e.table-wrap( class="wow  fadeInUp "  data-wow-delay=`${delay + .2}s` data-wow-duration=`${dur }s`   )
				table
					thead
						th(scope="col")
						th(scope="col") Минимум
						th.bg-white(scope="col") Лайт
						th(scope="col").bg-accent Базовый
						th.bg-white(scope="col") Оптимальный
						th(scope="col") VIP
					mixin tr(t1,t2,t3,t4,t5,t6)
						tr
							th(scope="row")!= t1
							td!= t2
							td.bg-white!= t3
							td.bg-accent!= t4
							td.bg-white!= t5
							td!= t6
					tbody
						+tr('Камер','1','1', '2','3','4')
						+tr('ИнжеНер','-','1', '1','1','1')
						+tr('Оператор','1','1', '2','3','4')
						+tr('Графика эфирная','Нет','Да', 'Да','Да','Да')
						+tr('Высококачественный звук','Да','Да', 'Да','Да','Да')
						+tr('Заставки, перебивки','Нет','Нет', 'Нет','Да','Да')
						+tr('Создание лендинга','Нет','Нет', 'Нет','Нет','Да')
					tfoot
						+tr('Стоимость',`От <strong>15 000</strong>`,`От <strong>25 000</strong>`, `От <strong>55 000</strong>`,`От <strong>75 000</strong>`,`От <strong>100 000</strong>`)
			.text-center
				//- a.btn.btn-lg.btn-primary.px-5.link-modal( class="wow fadeInUp" data-wow-delay=`${delay + .2}s` data-wow-duration=`${dur }s` href="https://formdesigner.ru/form/view/151680",target="_blank",onclick="FDPopup.open(151680,{'hideCloseBtn':false,'host':'formdesigner.ru','overlay':{'background':'#000000','opacity':0.1}}); return false;") РАССЧИТАТЬ  точную стоимость
				a.btn.btn-lg.btn-primary.px-5.link-modal( class="wow fadeInUp" data-wow-delay=`${delay + .2}s` data-wow-duration=`${dur }s` href="#modal-call") РАССЧИТАТЬ  точную стоимость
	// end sPrices
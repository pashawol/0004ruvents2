mixin sClients()
	// start sClients
	+b.SECTION.sClients.section#sClients&attributes(attributes)
		.container
			+b.section-title
				h2 Клиенты
				
			+e.row.row
				- for (var x = 1; x < 14; x++)
					+e.col.col-xl-2.col-lg-3.col-md-3.col-4( class="wow zoomIn "  data-wow-delay=`${delay + .2}s` data-wow-duration=`${dur }s`   )
						+e.img-wrap.hex
							+pic(`logo-${x}.png`,'','lazy','')(class="") 
	// end sClients